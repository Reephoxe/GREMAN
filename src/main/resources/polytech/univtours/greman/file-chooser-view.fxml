<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="polytech.univtours.greman.FileChooserController">
    <children>
      <Button layoutX="25.0" layoutY="25.0" mnemonicParsing="false" onAction="#backToMainScene" text="Retour" />
        <VBox alignment="CENTER" layoutY="56.0" prefHeight="244.0" prefWidth="600.0" spacing="10.0">
            <children>
                <Label contentDisplay="CENTER" prefHeight="62.0" prefWidth="185.0" text="Charger un fichier" textAlignment="CENTER">
                    <font>
                        <Font size="22.0" />
                    </font>
                </Label>
                <Button mnemonicParsing="false" onAction="#selectFile" text="Choisir un fichier" />
            </children>
        </VBox>
    </children>

</AnchorPane>
